<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.whatsup2.LoginController">
   <children>
      <Label layoutX="181.0" layoutY="29.0" text="Whats-UP">
         <font>
            <Font name="Bauhaus 93" size="59.0" />
         </font>
      </Label>
      <Label layoutX="260.0" layoutY="97.0" text="Login">
         <font>
            <Font name="Bauhaus 93" size="34.0" />
         </font>
      </Label>
      <TextField fx:id="UserPhone" layoutX="73.0" layoutY="150.0" prefHeight="31.0" prefWidth="454.0" promptText="Telefono">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button layoutX="269.0" layoutY="295.0" mnemonicParsing="false" onAction="#OnLoginClick" style="-fx-border-color: black;" text="Login">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Button layoutX="21.0" layoutY="17.0" mnemonicParsing="false" onAction="#OnExitClick" text="&lt;">
         <font>
            <Font name="Arial Black" size="20.0" />
         </font>
      </Button>
      <TextField fx:id="Password" layoutX="73.0" layoutY="203.0" prefHeight="26.0" prefWidth="454.0" promptText="Ruta de Certificado">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <VBox alignment="CENTER" layoutX="2.0" layoutY="252.0" prefHeight="31.0" prefWidth="594.0">
         <children>
            <Label fx:id="WarningLabel" text="Label">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="381.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.whatsup2.RegisterController">
   <children>
      <Label layoutX="181.0" layoutY="29.0" text="Whats-UP">
         <font>
            <Font name="Bauhaus 93" size="59.0" />
         </font>
      </Label>
      <Label layoutX="239.0" layoutY="97.0" style="-fx-text-fill: crimson;" text="Registro">
         <font>
            <Font name="Bauhaus 93" size="34.0" />
         </font>
      </Label>
      <TextField fx:id="UserName" layoutX="73.0" layoutY="150.0" prefHeight="31.0" prefWidth="454.0" promptText="Nombre de Usuario">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="UserPhon" layoutX="73.0" layoutY="200.0" prefHeight="31.0" prefWidth="454.0" promptText="Telefono">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button layoutX="275.0" layoutY="291.0" mnemonicParsing="false" onAction="#OnRegisterClick" style="-fx-text-fill: crimson; -fx-border-color: crimson;" text="Registrar">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <TextField fx:id="Password" layoutX="73.0" layoutY="247.0" prefHeight="31.0" prefWidth="454.0" promptText="Ruta a Certificado Digital">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button layoutX="21.0" layoutY="17.0" mnemonicParsing="false" onAction="#OnExitClick" text="&lt;">
         <font>
            <Font name="Bauhaus 93" size="20.0" />
         </font>
      </Button>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="325.0" prefHeight="39.0" prefWidth="600.0">
         <children>
            <Label fx:id="WarningLabel" style="-fx-text-fill: crimson;" textAlignment="CENTER">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
